<h1>Books#edit</h1>
<p>Find me in app/views/books/edit.html.erb</p>
<h3>ブック編集</h3>
<%= form_with model: @book do |f| %>
<table>
  <tr>
    <th><%= f.label :title ,"タイトル" %></th>
    <td><%= f.text_field :title %></td>
  </tr>
  <tr>
    <th><%= f.label :address ,"住所" %></th>
    <td><%= f.text_field :address %></td>
  </tr>
  <tr>
    <th><%= f.label :content ,"内容" %></th>
    <td><%= f.text_area :content %></td>
  </tr>
</table>
<div>
  <ul>
    <li><%= f.submit "編集を完了する" %></li>
    <li><%= link_to "ブック情報一覧に戻る", :root %></li>
  </ul>
</div>
<% end %>