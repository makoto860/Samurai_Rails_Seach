<h1>Books#new</h1>
<p>Find me in app/views/books/new.html.erb</p>
<h3>ブック新規作成</h3>
<%= form_with model: @book do |f| %>
<table>
  <tr>
    <th><%= f.label :title ,"タイトル" %></th>
    <td><%= f.text_field :title %></td>
  </tr>

  <tr>
    <th><%= f.label :address ,"住所" %></th>
    <td><%= f.text_field :address %></td>
  </tr>

  <tr>
    <th><%= f.label :content ,"内容" %></th>
    <td><%= f.text_area :content %></td>
  </tr>
</table>
<div>
  <ul>
    <li><%= f.submit "登録完了する"%></li>
    <li><%= link_to "ブック情報一覧に戻る", :books %></li>
  </ul>
</div>
<% end %>
